{
  "name": "dynamic-import-module",
  "version": "0.0.1",
  "description": "PoC to import an ES6 module into vanilla js",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --config ./config/webpack/webpack.standalone.dev.js",
    "build:dev": "npm run build:standalone:dev && npm run build:module:dev",
    "build:module:dev": "rimraf build/lib && webpack --config ./config/webpack/webpack.module.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Oscar Garcia Masia",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
